{
    "name": "luisdalmolin/laravel-historiable",
    "description": "A Laravel package to easily keep history of your models",
    "license": "MIT",

    "require": {
    },

    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "mockery/mockery": "0.9.*",
        "orchestra/testbench": "3.4.*",
        "orchestra/testbench-browser-kit": "3.4.*",
        "doctrine/dbal": "~2.3"
    },

    "autoload": {
        "psr-4": {
            "Dalmolin\\Historiable\\": "src/"
        },

        "files": [
            "src/functions.php"
        ]
    },

    "autoload-dev": {
        "classmap": []
    },

    "extra": {
        "branch-alias": {
            "dev-master": "0.1.*-dev"
        }
    },

    "minimum-stability": "dev",
    "prefer-stable": true
}